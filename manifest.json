{
    "name":"AdGuardio",
    "version":"1.0",
    "description":"Blocks ads from certain domains",
    "author":"Shashank Srivastava",
    "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "declarativeNetRequestFeedback"
     ],
    "host_permissions": [
    "*://*/*"
  ],
    "background":{
        "service_worker":"background.js"
    },
    "icons":{
        "16":"icons/logo1.png",
        "48":"icons/logo2.png",
        "128":"icons/logo3.png"
    },
    "manifest_version":3

}